{
  "name": "@process-flow/frontend",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:css": "stylelint \"src/**/*.{css,scss}\"",
    "lint:css:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run format && npm run lint:css:fix",
    "imports:organize": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "check-all": "npm run type-check && npm run lint && npm run format:check",
    "preview": "vite preview",
    "publish:types": "yarn build:types && npm publish",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=vitest-sonar-reporter"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.3.6",
    "@process-flow/common": "workspace:*",
    "@react-rxjs/core": "^0.10.8",
    "@react-rxjs/utils": "^0.9.7",
    "@reduxjs/toolkit": "^2.7.0",
    "@xyflow/react": "^12.5.1",
    "antd": "^5.27.4",
    "axios": "^1.8.4",
    "formik": "^2.4.6",
    "lodash.debounce": "^4.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^5.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.11.0",
    "rxjs": "^7.8.2",
    "sass": "^1.86.3",
    "uuid": "^11.1.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "^18.2.53",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "stylelint": "^16.21.1",
    "stylelint-config-standard-scss": "^15.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-sonar-reporter": "^2.0.1"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mannuluss/process-flow-ui.git"
  },
  "main": "dist/process-flow-types.umd.js",
  "module": "dist/process-flow-types.es.js",
  "types": "dist/types/exported-types.d.ts",
  "files": [
    "dist"
  ]
}
